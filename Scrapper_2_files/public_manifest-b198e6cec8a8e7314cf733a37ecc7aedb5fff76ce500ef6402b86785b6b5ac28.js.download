  componentDidMount: function () {
    this.detectWebgl();
    this.detectIos();

    if (this.props.is_eox) {
      this.getInitialData();
      this.getObservatories();
    } else {
      this.getObservatories();
      this.onEnter();
    }

    var that = this;
    $('body').on("close_menus", function () {
      that.closeHeaderSubNav();
    });
