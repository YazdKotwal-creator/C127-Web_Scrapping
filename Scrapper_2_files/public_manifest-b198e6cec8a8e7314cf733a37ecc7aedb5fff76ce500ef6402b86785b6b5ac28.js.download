                  var re = that.props.model == "events" ? /^(\d+)(%3A|:)start_date/ : /^(\d+)(%3A|:)publish_date/;
                  var match = Array.isArray(date_param) ? date_param[0].match(re) : date_param.match(re);
                  param = match ? match[1] : null;
                }
