      var lng_cardinal = this.state.lng > 0 ? 'E' : 'W';
      return Math.abs(this.state.lat.toFixed(2)) + '° ' + lat_cardinal + ', ' + Math.abs(this.state.lng.toFixed(2)) + '° ' + lng_cardinal;
